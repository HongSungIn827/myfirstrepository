package DB;

public class Employee {
	public int EMPNO;
	public String ENAME;
	public String JOB;
	public int MGR;
	public String HIREDATE;
	public int SAL;
	public int COMN;
	public int DEPTNO;
	
	public Employee(int eno, int m, int s, int c, int dno, String ena, String j, String h){
		EMPNO = eno;
		ENAME = ena;
		JOB = j;
		MGR = m;
		HIREDATE = h;
		SAL = s;
		COMN = c;
		DEPTNO = dno;
	}
}

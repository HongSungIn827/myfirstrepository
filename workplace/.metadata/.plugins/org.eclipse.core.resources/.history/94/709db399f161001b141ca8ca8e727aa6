
public class Ex01 {
	public static void main(String[] args) {
		
		int num1 = 23;
		int num2 = 7;
		
		System.out.println("num1 + num2 = "+num1+num2);
		System.out.println(num1+" + "+num2+" = "+(num1+num2));
		System.out.println(num1+" - "+num2+" = "+(num1-num2));
		System.out.println(num1+" * "+num2+" = "+(num1*num2));
		System.out.println(num1+" / "+num2+" = "+(num1/(double)num2));
		System.out.println(num1+" % "+num2+" = "+(num1%num2));
	}

}

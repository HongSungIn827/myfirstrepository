class StaticValues{
	static int A = 10; // 표준 규율
}
class A{
	public void doPrintStaticA(){System.out.println("SV.A = "+StaticValues.A++);}
}
class B{
	public void doPrintStaticB(){System.out.println("SV.A = "+StaticValues.A++);}
}
public class Ex01 {
	public static void main(String[] agrs){
		
	}
}

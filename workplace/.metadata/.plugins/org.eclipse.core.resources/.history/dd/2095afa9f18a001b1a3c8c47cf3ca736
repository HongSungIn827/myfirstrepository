
public class Test {
	public int sum(int a,int b){
		return a+b;
	}
	
	public String say(){
		return "Hi";
	}
	
	public static void main(String[] args){
		int a = 3;
		int b = 4;
		
		Test myTest = new Test();
		int c = myTest.sum(a, b);
		
		Test myTest1 = new Test();
		String d = myTest1.say();
		
		System.out.println(c);
		System.out.println(d);
	}
}
